{
  "name": "lbh-frontend-react",
  "version": "0.4.0",
  "description": "London Borough of Hackney's React component library",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "LBHackney-IT/lbh-frontend-react",
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "npm run build:barrels && rollup --config",
    "build:watch": "npm run build -- --watch",
    "build:barrels": "barrelsby --config ./barrelsby.json",
    "build:docs": "typedoc",
    "build-lib-tsc-watch": "tsc -w",
    "build-lib-tsc": "tsc",
    "test": "npm run format:check && npm run lint && npm run lint:codeowners && npm run test:all",
    "test:ci": "npm run format:check && npm run lint && npm run lint:codeowners && npm run test:all:ci",
    "test:update": "npm run format && npm run lint:fix && npm run lint:codeowners && npm run test:all:update",
    "test:all": "npm run build && npm run test:unit",
    "test:all:ci": "npm run build && npm run test:unit:ci",
    "test:all:update": "npm run build && npm run test:unit:update",
    "test:unit": "jest",
    "test:unit:ci": "npm run test:unit -- --maxWorkers 1",
    "test:unit:update": "npm run test:unit -- --updateSnapshot",
    "test:coverage": "npm test -- --coverage",
    "eject-ts": "react-scripts-ts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint --report-unused-disable-directives --config ./.eslintrc.full.js '**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:codeowners": "codeowners-enforcer -i 'package.json' -i 'package-lock.json'",
    "format": "prettier --write '**/*'",
    "format:check": "prettier --check '**/*'",
    "prepack": "npm run build",
    "preversion": "npm install && npm test && bin/check-git"
  },
  "peerDependencies": {
    "react": ">= 16"
  },
  "dependencies": {
    "@storybook/preset-typescript": "^3.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-table": "^7.0.19",
    "@types/react-tabs": "^2.3.2",
    "bootstrap-icons": "^1.0.0-alpha5",
    "classnames": "^2.2.6",
    "moment": "^2.27.0",
    "null-as-undefined": "^0.2.1",
    "prop-types": "^15.7.2",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.2.2",
    "react-tabs": "^3.1.1",
    "sass-loader": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime-corejs2": "^7.9.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.7.7",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.22.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.1.0",
    "barrelsby": "^2.2.0",
    "codeowners-enforcer": "^1.0.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-jsdoc": "^23.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.4.0",
    "govuk-frontend": "^3.6.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.5.4",
    "lbh-frontend": "^3.0.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.12.0",
    "rollup": "^1.28.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.1.0",
    "rollup-plugin-multi-input": "^1.0.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-progress": "^1.1.1",
    "rollup-plugin-typescript2": "^0.25.3",
    "ts-jest": "^25.2.1",
    "ts-loader": "^7.0.5",
    "tslib": "^1.11.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.9.6"
  },
  "husky": {
    "pre-commit": "pretty-quick --staged"
  }
}
